<resources>

  <!-- Base application theme. -->
  <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
  </style>

  <style name="AppTheme.Exclude" parent="AppTheme">
    <item name="android:windowEnterTransition">@transition/exclude_transition</item>
  </style>

  <style name="AppTheme.Customize" parent="AppTheme">
    <item name="android:windowSharedElementEnterTransition">@transition/playstore_transition</item>
    <item name="android:windowSharedElementsUseOverlay">true</item>
  </style>

</resources>
